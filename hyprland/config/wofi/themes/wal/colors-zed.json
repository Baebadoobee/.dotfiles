{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#28224090",
                "editor.background": "#28224090",
                "editor.foreground": "#c9c7cf",
                "text": "#c9c7cf",
                "text.muted": "#c9c7cf70",
                "text.ignored": "#c9c7cf40",
                "text.placeholder": "#c9c7cf50",
                "ignored": "#c9c7cf30",
                "element.hover": "#c9c7cf30",
                "ghost_element.hover": "#4E9BFA30",
                "ghost_element.selected": "#4E9BFA30",
                "ghost_element.active": "#4E9BFA60",
                "border": "#282240",
                "editor.highlighted_line.background": "#4E9BFA10",
                "editor.active_line.background": "#4E9BFA10",
                "panel.background": "#28224090",
                "title_bar.background": "#28224090",
                "title_bar.inactive_background": "#28224090",
                "status_bar.background": "#28224090",
                "drop_target.background": "#28224090",
                "elevated_surface.background": "#282240",
                "toolbar.background": "#28224090",
                "tab_bar.background": "#28224090",
                "tab.inactive_background": "#28224090",
                "tab.active_background": "#4E9BFA30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#282240",
                "scrollbar.thumb.background": "#4E9BFA",
                "editor.gutter.background": "#28224090",
                "terminal.background": "#28224010",
                "terminal.foreground": "#c9c7cf",
                "terminal.dim_foreground": "#c9c7cf",
                "terminal.bright_foreground": "#c9c7cf",
                "terminal.ansi.black": "#282240",
                "terminal.ansi.red": "#4E9BFA",
                "terminal.ansi.green": "#689AF3",
                "terminal.ansi.yellow": "#6EAFFB",
                "terminal.ansi.blue": "#9796B5",
                "terminal.ansi.magenta": "#93A7E6",
                "terminal.ansi.cyan": "#8DB9F8",
                "terminal.ansi.white": "#c9c7cf",
                "terminal.ansi.bright_black": "#716c84",
                "terminal.ansi.bright_red": "#4E9BFA",
                "terminal.ansi.bright_green": "#689AF3",
                "terminal.ansi.bright_yellow": "#6EAFFB",
                "terminal.ansi.bright_blue": "#9796B5",
                "terminal.ansi.bright_magenta": "#93A7E6",
                "terminal.ansi.bright_cyan": "#8DB9F8",
                "terminal.ansi.bright_white": "#c9c7cf",
                "modified": "#4E9BFA",
                "syntax": {
                    "attribute": {
                        "color": "#4E9BFA"
                    },
                    "boolean": {
                      "color": "#689AF3"
                    },
                    "comment": {
                      "color": "#716c84"
                    },
                    "comment.doc": {
                      "color": "#716c84"
                    },
                    "constant": {
                      "color": "#689AF3"
                    },
                    "function": {
                      "color": "#8DB9F8"
                    },
                    "keyword": {
                      "color": "#9796B5"
                    },
                    "number": {
                      "color": "#93A7E6"
                    },
                    "operator": {
                      "color": "#9796B5"
                    },
                    "string": {
                      "color": "#4E9BFA"
                    },
                    "variable": {
                      "color": "#689AF3"
                    }
                },
                "players": [
                  {
                    "cursor": "#4E9BFA",
                    "background": "#282240",
                    "selection": "#4E9BFA30"
                  }
                ]
            }
        }
    ]
}
